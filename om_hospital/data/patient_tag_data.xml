<odoo>
<!--    <data noupdate="0">  -->
<!--      by default in odoo noupdate=false which main any update in record will be excute
            to modify must use noupdate="1"-->
    <data noupdate="1">
        <record id="patient_1" model="patient.tag" forcecreate="0">
<!--            forcecreate="0" :means that when you delet the record will not be created again ever-->
            <field name="name">Ali</field>
            <field name="active" eval="True"/>
        </record>

        <record id="patient_2" model="patient.tag">
            <field name="name">Ali</field>
            <field name="color">4</field>
        </record>

        <record id="patient_3" model="patient.tag">
            <field name="name">Doha</field>
            <field name="color">5</field>
            <field name="active" eval="False"/>
        </record>

        <record id="patient_4" model="patient.tag">
            <field name="name">Nada</field>
            <field name="color">2</field>
            <field name="active" eval="True"/>
        </record>
    </data>

</odoo>